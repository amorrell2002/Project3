{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileBasename}",
                "-g",
                "./County_Class/*.cpp",
                "-g",
                "./Merge_Sort/*.cpp",
                "-I",
                "./Merge_Sort",
                "-L",
                "./Merge_Sort",
                "-I",
                "./County_Class/nlohmann",
                "-I",
                "./County_Class",
                "-L",
                "./County_Class",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}